// 既定値を上書きするには、このファイル内にキー バインドを挿入します
[
    {
        "key": "tab",
        "command": "emacs-tab.reindentCurrentLine",
        "when": "editorTextFocus"
    },
    {
        "key": "ctrl+e",
        "command": "cursorEnd",
        "when": "editorTextFocus"
    },
    {
        "key": "ctrl+a",
        "command": "cursorHome",
        "when": "textInputFocus"
    }
]
