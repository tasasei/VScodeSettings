// 既定値を上書きするには、このファイル内にキー バインドを挿入します
[
    {
        "key": "ctrl+e",
        "command": "cursorEnd"
    },
    {
        "key": "ctrl+a",
        "command": "cursorHome",
        "when": "textInputFocus"
    },
    {
        "key": "ctrl+tab",
        "command": "workbench.action.nextEditor"
    },
    {
        "key": "ctrl+pagedown",
        "command": "-workbench.action.nextEditor"
    },
    {
        "key": "ctrl+shift+tab",
        "command": "workbench.action.previousEditor"
    },
    {
        "key": "ctrl+pageup",
        "command": "-workbench.action.previousEditor"
    },
    {
        "key": "escape shift+oem_comma",
        "command": "cursorTop",
        "when": "textInputFocus"
    },
    {
        "key": "escape shift+oem_period",
        "command": "cursorBottom",
        "when": "textInputFocus"
    }
]
